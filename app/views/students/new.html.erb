<h3>Student Form</h3>
  <form action="<%= students_path %>" method="post">
    <%= form_tag students_path do %>
    <label>First name: </label>
    <input type="text" id="first_name" name="student[first_name]"<br>
    <!--<%= text_field_tag :'student[first_name]' %><br>-->
    <label>Last name: </label>
    <input type="text" id="last_name" name="student[last_name]"<br>
    <!--<%= text_field_tag :'student[last_name]' %><br>-->
      <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type="submit" value="Submit Student">
    <!--<%= submit_tag "Submit Post" %>-->
    <% end %>
  </form>
<%= params.inspect %>
